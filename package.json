{
  "name": "simplechart",
  "version": "1.0.0",
  "description": "Visualization of existing map based stats",
  "main": "index.js",
  "scripts": {
    "start": "nf start",
    "start:dev": "webpack-dev-server --config ./config/webpack/dev.js",
    "start:prod": "cross-env REST_ENV=real NODE_ENV=production webpack-dev-server --config ./config/webpack/prod.js",
    "build": "npm run build:dev",
    "build:dev": "rimraf dist && webpack --config ./config/webpack/dev.js",
    "build:prod": "rimraf dist && cross-env REST_ENV=real NODE_ENV=production webpack -p --config ./config/webpack/prod.js",
    "lint": "tslint -c ./tslint.json --force src/**/*.ts src/**/*.tsx",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose -i",
    "test:coverage": "jest --verbose --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lemoncode/simplechart.git"
  },
  "author": "Lemoncode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lemoncode/simplechart"
  },
  "homepage": "https://github.com/Lemoncode/simplechart#readme",
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "d3": "^5.4.0",
    "d3-composite-projections": "^1.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^3.2.1",
    "topojson-client": "^3.0.0",
    "topojson-simplify": "^3.0.2"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/deep-freeze": "^0.1.1",
    "@types/enzyme": "^3.1.9",
    "@types/geojson": "^7946.0.4",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.6",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^3.0.15",
    "@types/redux-mock-store": "0.0.13",
    "@types/topojson-client": "^3.0.0",
    "@types/topojson-simplify": "^3.0.0",
    "@types/topojson-specification": "^1.0.0",
    "@types/webpack-env": "^1.13.5",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "foreman": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.4",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "raf": "^3.4.0",
    "redux-mock-store": "1.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.3.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "testRegex": "\\.spec\\.tsx?$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "setupFiles": [
      "<rootDir>/config/test/polyfills.js",
      "<rootDir>/config/test/setupTest.js"
    ],
    "transform": {
      ".tsx?": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.s?css$": "identity-obj-proxy"
    },
    "restoreMocks": true
  }
}
